{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/sw.js",
      "dest": "/sw.js",
      "headers": {
        "Content-Type": "application/javascript",
        "Service-Worker-Allowed": "/"
      }
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json",
      "headers": {
        "Content-Type": "application/json"
      }
    },
    {
      "src": "/icon-(.*)\\.png",
      "dest": "/icon-$1.png",
      "headers": {
        "Content-Type": "image/png"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
